#create funciton that calcuates BMI and returns weight category

def bmi(weight, height):
    bmi = weight/height**2
    if bmi <= 18.5:
        return("Underweight")
    elif bmi <= 25.0 and bmi > 18.5: 
        return("Normal")
    elif bmi <= 30.0 and bmi > 25.0:
        return("Overweight")
    else:
        return("Obese")
